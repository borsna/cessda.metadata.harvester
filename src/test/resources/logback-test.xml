<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<contextName>eqbh</contextName>
	<appender name="STDOUT"
		class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{yyyy-dd-MM HH:mm:ss.SSS} [%-8.8thread]  %highlight(%-5.5level) %-30.30((%M\)) %40.40((%file:%line\)) - %message%n
			</pattern>
		</encoder>
	</appender>
	<root level="INFO">
		<appender-ref ref="STDOUT" />
	</root>
</configuration>
